{
  "expo": {
    "name": "MYFT",
    "slug": "myft-mobile-clean",
    "version": "1.0.18",
    "orientation": "portrait",
    "scheme": "myftmobileapp",
    "icon": "./images/MYFT_APP_LOGO.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "assetBundlePatterns": [
      "**/*"
    ],
    "splash": {
      "image": "./images/MYFT_APP_LOGO.png",
      "resizeMode": "contain",
      "backgroundColor": "#00274C"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.backjaum33.myftmobileclean",
      "requireFullScreen": false,
      "infoPlist": {
        "NSCameraUsageDescription": "MYFT uses the camera to capture profile photos.",
        "NSPhotoLibraryUsageDescription": "MYFT needs access to your photo library to select profile pictures.",
        "NSPhotoLibraryAddUsageDescription": "MYFT can save photos to your library.",
        "UIStatusBarStyle": "UIStatusBarStyleLightContent",
        "UIViewControllerBasedStatusBarAppearance": false,
        "CFBundleDisplayName": "MYFT",
        "ITSAppUsesNonExemptEncryption": false,
        "UILaunchStoryboardName": "SplashScreen",
        "UIRequiredDeviceCapabilities": [
          "armv7"
        ],
        "UISupportedInterfaceOrientations": [
          "UIInterfaceOrientationPortrait"
        ],
        "UISupportedInterfaceOrientations~ipad": [
          "UIInterfaceOrientationPortrait",
          "UIInterfaceOrientationPortraitUpsideDown",
          "UIInterfaceOrientationLandscapeLeft",
          "UIInterfaceOrientationLandscapeRight"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./images/MYFT_APP_LOGO.png",
        "backgroundColor": "#00274C"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "owner": "backjaum33",
    "extra": {
      "eas": {
        "projectId": "9ce50732-f833-47e7-99e3-8e389d2aaec6"
      }
    },
    "plugins": [
      "expo-font"
    ]
  }
}
